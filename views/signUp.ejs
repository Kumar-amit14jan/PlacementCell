<link rel="stylesheet" href="/css/signUp.css">
<nav>
    <a href="/">Sign In</a>
</nav>
<section>
    <h1>Sign Up To Account</h1>
    <form id ="form" action="/create_session" method="POST">
        <input id="firstname" type=text name="firstname" placeholder="Enter First Name"><i class="fa-solid fa-circle-check success"></i><i class="fa-solid fa-circle-exclamation error"></i><br><small class="error">error</small><br>
        <input id="lastname" type=text name="lastname" placeholder="Enter Last Name"><i class="fa-solid fa-circle-check success"></i>&nbsp;<i class="fa-solid fa-circle-exclamation error"></i><br><small class="error">error</small><br>
        <input id="email" type=text name="email" placeholder="Enter The Email"><i class="fa-solid fa-circle-check"></i>&nbsp;<i class="fa-solid fa-circle-exclamation"></i><br><br>
        <input id="password" type=password name="password" placeholder="Enter The Password"><i class="fa-solid fa-circle-check"></i>&nbsp;<i class="fa-solid fa-circle-exclamation"></i><br><br>
        <button id="button" type="submit">Submit</button>
        

    </form>
</section>

