<h1>This is interview list</h1>
<div>
    <table>
        <tr>
            <th>SNO.</th>
            <th>CompanyName</th>
            <th>Action</th>
        </tr>
        <% for(var i = 0 ; i<interview_list.length ; i++) { %>
            <tr>
                <td><%= i + 1 %></td>
                <td><%= interview_list[i].companyName %></td>
                <td><button><a href="/result/<%= interview_list[i].id %>">Mark Result</a></button></td>
            </tr>
        <% } %>
    </table>
</div>